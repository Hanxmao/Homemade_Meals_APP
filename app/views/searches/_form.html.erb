<%= search_form_for(@q, url: search_path, method: :get, class:"d-flex") do |f| %>
    <%= f.search_field :title_or_ingredients_or_user_username_cont, 
            placeholder: "Search Meal",
            class: "form-control me-2" %>
    <%= f.submit "Search", class:"btn btn-outline-dark" %>  
<% end %>  